{
  "compilerOptions": {
    "types": ["node", "jest"],
    "target": "ES6",
    "module": "CommonJS", 
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
}
