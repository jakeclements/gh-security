{
  "name": "gh-security",
  "version": "0.0.0-development",
  "description": "Outputs a table ranking Github Dependabot security vulnerability warnings",
  "bin": {
    "gh-security": "bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "start": "ts-node ./",
    "build": "rimraf ./bin && tsc && chmod +x ./bin/index.js",
    "semantic-release": "semantic-release",
    "dry:semantic-release": "NPM_TOKEN=$NPM_TOKEN GH_TOKEN=$GH_TOKEN semantic-release"
  },
  "author": {
    "name": "Jake Clements",
    "email": "jakeclements@users.noreply.github.com"
  },
  "license": "ISC",
  "dependencies": {
    "@octokit/core": "^4.0.5",
    "chalk": "^5.0.1",
    "cli-table": "^0.3.11",
    "commander": "^9.4.1",
    "conf": "^10.2.0",
    "loading-cli": "^1.1.0",
    "multisort": "^0.5.3",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@octokit/types": "^7.5.1",
    "@semantic-release/changelog": "^6.0.1",
    "@types/cli-table": "^0.3.1",
    "@types/jest": "^29.2.2",
    "@types/multisort": "^0.5.0",
    "@types/node": "^18.7.23",
    "jest": "^29.2.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakeclements/gh-security.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
